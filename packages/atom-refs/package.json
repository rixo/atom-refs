{
  "name": "atom-refs",
  "main": "./lib/main",
  "version": "0.0.3",
  "author": {
    "name": "rixo",
    "email": "rixo@rixo.fr"
  },
  "description": "Navigate and refactor references",
  "keywords": [],
  "repository": "https://github.com/rixo/atom-references",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "atomTestRunner": "./spec/test-runner",
  "activationCommands": [],
  "activationHooks": [
    "language-javascript:grammar-used",
    "language-typescript:grammar-used",
    "language-babel:grammar-used",
    "language-php:grammar-used",
    "language-python:grammar-used",
    "ide-svelte:grammar-used"
  ],
  "consumedServices": {
    "vim-mode-plus": {
      "versions": {
        "^0.1.0": "consumeVimModePlus"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeIndie"
      }
    }
  },
  "scripts": {
    "prettier": "prettier --write lib/**/*.js"
  },
  "dependencies": {
    "@babel/parser": "^7.4.4",
    "atom-refs-escope": "file:../escope",
    "babel-traverse": "^6.26.0",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0",
    "debug": "^3.1.0",
    "php-parser": "^3.0.0-alpha2",
    "svelte": "^3.4.1",
    "tree-sitter": "^0.12.8",
    "tree-sitter-python": "^0.11.3",
    "underscore-plus": "^1.6.6"
  },
  "devDependencies": {
    "@rixo/eslint-config-atom": "^0.5.0",
    "dedent": "^0.7.0",
    "eslint-plugin-disable": "^1.0.4"
  }
}

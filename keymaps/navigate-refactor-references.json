{
  "atom-workspace atom-text-editor.vim-mode-plus": {
    "alt-k": "atom-refs:previous",
    "alt-j": "atom-refs:next"
  },
  "atom-workspace atom-text-editor.vim-mode-plus.normal-mode, atom-workspace atom-text-editor.vim-mode-plus.insert-mode": {
    "ctrl-r": "atom-refs:select"
  },
  "atom-text-editor.vim-mode-plus:not(.insert-mode)": {
    "g d": "atom-refs:jump-to-local",
    "g D": "atom-refs:jump-to-final"
  }
}
